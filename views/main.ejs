    <% if(message === "success") { %>
        <script>
            alert("You API Key is <%= key %>")
            window.location.href = "/"
        </script>
    <% } %>

    <% if(message === "duplicate") { %>
        <script>
            alert("The Email is already registered. Please Check your old mails for the API Key.")
            window.location.href = "/"
        </script>
    <% } %>

    <% if(message === "extension") { %>
        <script>
            alert("The extension of your email address's domain is not supported by our api! We suggest you to use a email address issued by gmail.")
            window.location.href = "/"
        </script>
    <% } %>

    <% if(message === "domain") { %>
        <script>
            alert("The domain of you email address is not supported by our api! We suggest you to use a email address issued by gmail.")
            window.location.href = "/"
        </script>
    <% } %>

